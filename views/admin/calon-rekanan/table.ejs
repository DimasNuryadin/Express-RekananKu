<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama Perusahaan</th>
      <th>Bidang Usaha</th>
      <th>Status</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i=0; i < statusRekanan.length; i++) { %>
      <tr>
        <td>
          <%= i + 1 %>
        </td>
        <td>
          Bidang Usaha
        </td>
        <td>
          Nama Perusahaan
        </td>
        <td>
          <%= statusRekanan[i].status %>
        </td>
        <td>
          <form action="">
            <a class="btn btn-light" href="/calon-rekanan/view-user">Lihat</a>
            <button class="btn btn-success">Terima</button>
          </form>
        </td>
      </tr>
      <% } %>
  </tbody>
</table>