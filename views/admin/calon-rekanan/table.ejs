<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama Perusahaan</th>
      <th>Bidang Usaha</th>
      <th>Status</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i=0; i < statusRekanan.length; i++) { %>
      <tr>
        <td>
          <%= i + 1 %>
        </td>
        <td>
          <%= statusRekanan[i].data_perusahaan.namaPerusahaan %>
        </td>
        <td>
          <%= statusRekanan[i].data_perusahaan.bidangUsaha %>
        </td>
        <td>
          <span class="badge bg-info text-white">
            <%= statusRekanan[i].status %>
          </span>
        </td>
        <td class="d-flex gap-5">
          <a class="btn btn-primary mr-2"
            href="/admin/calon-rekanan/view-user/<%= statusRekanan[i].user_id %>">Lihat</a>
          <form action="/admin/calon-rekanan/accept/<%= statusRekanan[i]._id %>?_method=PUT" method="POST" class="mr-2">
            <button class="btn btn-success">Terima</button>
          </form>
          <form action="/admin/calon-rekanan/reject/<%= statusRekanan[i]._id %>?_method=PUT" method="POST">
            <button class="btn btn-danger">Tolak</button>
          </form>
        </td>
      </tr>
      <% } %>
  </tbody>
</table>